/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:10rpx 0;
  box-sizing: border-box;
} 
/* 字体 */
/* 我的发现小程序音乐社推荐歌单排行电台 */
@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_hfc12xr5fk5.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_hfc12xr5fk5.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_hfc12xr5fk5.woff2') format('woff2'),
  url('//at.alicdn.com/t/webfont_hfc12xr5fk5.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_hfc12xr5fk5.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_hfc12xr5fk5.svg#杨任东竹石体-Bold') format('svg'); /* iOS 4.1- */
}

/* 微信小程序图标 */
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1576504_za0eexjv53.eot?t=1577000057700'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1576504_za0eexjv53.eot?t=1577000057700#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1576504_za0eexjv53.woff?t=1577000057700') format('woff'),
  url('//at.alicdn.com/t/font_1576504_za0eexjv53.ttf?t=1577000057700') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1576504_za0eexjv53.svg?t=1577000057700#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color:white;
}

